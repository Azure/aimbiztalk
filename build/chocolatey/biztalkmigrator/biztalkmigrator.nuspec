<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2015/06/nuspec.xsd">
  <metadata>
    <id>biztalkmigrator</id>
    <!-- Note This version will be overriden by the command line. -->
    <version>0.0.1-beta</version>
    <packageSourceUrl>https://github.com/azure/aimbiztalk</packageSourceUrl>
    <title>BizTalk Migrator Tool</title>
    <authors>Microsoft</authors>
    <projectUrl>https://github.com/azure/aimbiztalk</projectUrl>
    <projectSourceUrl>https://github.com/azure/aimbiztalk</projectSourceUrl>
    <bugTrackerUrl>https://github.com/azure/aimbiztalk/issues</bugTrackerUrl>
    <licenseUrl>https://github.com/azure/aimbiztalk/blob/master/LICENSE</licenseUrl>
    <docsUrl>https://github.com/azure/aimbiztalk#readme</docsUrl>
    <requireLicenseAcceptance>true</requireLicenseAcceptance>
    <description>Command Line Interface (CLI) to migrate BizTalk applications to Microsoft Azure Integration Services. This package includes the dependency on the Azure Integration Migration CLI and the Azure templates.</description>
    <tags>biztalkmigrator BizTalk Azure Integration AIM AIS CLI</tags>
    <summary>Command Line Interface (CLI) to migrate BizTalk applications to Microsoft Azure Integration Services. This package includes the dependency on the Azure Integration Migration CLI and the Azure templates.</summary>
    <dependencies>
      <dependency id="biztalkmigrator-cli"/>
      <dependency id="biztalkmigrator-azure"/>
    </dependencies>
  </metadata>
  <files>
    <file src="tools\**" target="tools" />
    <file src="..\..\..\dist\choco-src\*" target="tools\bin"/>
    <file src="..\..\..\templates\**" target="tools\templates"/>
    <file src="..\..\..\config\**" target="tools\config"/>
  </files>
</package>